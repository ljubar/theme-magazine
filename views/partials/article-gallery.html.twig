<script>
 var galleryLinks = [];

  {% gimmelist slideshow from slideshows with { article: gimme.article } %}
  {% gimmelist slideshowItem from slideshowItems|limit(99) with { article: gimme.article, slideshow: slideshow } %}
    {% gimme rendition with {'media': slideshowItem.articleMedia, 'name': '960x480', 'fallback': 'original' } %}
      galleryLinks.push({
         title: '{{ media.description }}',
         href: '{{ url(rendition) }}',
         type: 'image/jpeg'
      });
    {% endgimme %}
  {% endgimmelist %}
  {% endgimmelist %}
</script>
